<% #pins.transition-enabled %>
<% @pins.each_slice(4) do |row| %>
<div class="row">
  <% row.each do |pin| %>
    <div class="col-md-3">
      <div class="thumbnail">
<h2><%= link_to (image_tag pin.image.url), pin %></h2>
</div>
  <div class="name">
<h2><%= link_to pin.title, pin %></h2>
<p>Submitted by <%= pin.user.email %></p>
</div>
  </div>
<% end %>
</div>
<% end %>
